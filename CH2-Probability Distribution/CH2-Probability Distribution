\section*{Chapter 2 Probability Distribution}
\subsection*{Problem 8}
\begin{flushleft}

\begin{equation}
\begin{split}
E_y{}[E_{x}[x|y]] = \int \int xp(x|y) dx dy \\
= \int \int xp(x, y)/p(y) dx p(y) dy \\
= \int x \int p(x,y) dy dx \\
= \int x p(x) dx
= E[x] 
\end{split}
\end{equation}







\end{flushleft}


